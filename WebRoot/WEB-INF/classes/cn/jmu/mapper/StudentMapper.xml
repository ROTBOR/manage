<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.jmu.mapper.StudentMapper">
	
	<select id="findBySno" resultType="cn.jmu.po.Student" parameterType="String">
		select sno,userid,sname from student where sno=#{sno}
	</select>
</mapper>